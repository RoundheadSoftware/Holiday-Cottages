@inherits Umbraco.Web.Mvc.UmbracoTemplatePage 
    <!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@Model.Content.Name</title>
    <link href="~/Assets/Minified/css/main.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="wrapper">
        <div class="hero">@Html.Partial("Hero", Model)</div>
        <div class="nav">@Html.Partial("Navigation", Model)</div>
        @RenderBody()
        <div class="footer">@Html.Partial("Footer", Model)</div>
    </div>
    <script src="~/Assets/Minified/JavaScript/main.js"></script>
    <script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
</body>
</html>